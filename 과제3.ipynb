    "f = open(\"Text.txt\", 'r')\n",
    "nums = f.readlines()\n",
    "f.close()\n",
    "\n",
    "int_nums = list(map(lambda a: int(a), nums))\n",
    "\n",
    "print('전체 합은',sum(int_nums),'입니다')\n",
    "print('전체 평균은',round(sum(int_nums)/len(int_nums)),'입니다')\n"
